@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix retrct: <https://www.jmillanacosta.com/retractable/> .
@prefix retrct_terms: <https://www.jmillanacosta.com/retractable/schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix obo: <http://purl.obolibrary.org/obo> .
@prefix cito: <http://purl.org/spar/cito> .

obo:RO_0002410 a owl:Class ;
    rdfs:label "causally related to" .

retrct_terms:Retraction a owl:Class ;
    rdfs:label "Retraction" ;
    owl:sameAs wd:Q45203135 ;
    rdfs:comment "A retraction is the cause of a paper being retracted. Not all retractions involve or can be linked to retraction notices." ;
    cito:confirms retrct_terms:Retraction  .


retrct_terms:Paper a owl:Class ;
    rdfs:label "Paper" ;
    rdfs:subClassOf dcterms:BibliographicResource ;
    rdfs:comment "A research paper or academic journal article." .
    owl:sameAs wd:Q18918145 .

retrct_terms:RetractedPaper a owl:Class ;
    rdfs:label "Retracted Paper" ;
    rdfs:subClassOf retrct_terms:Paper ;
    rdfs:comment "A paper that has been retracted." ;
    owl:sameAs wd:Q45182324 ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty retrct_terms:hasRetractionNotice ;
        owl:someValuesFrom retrct_terms:RetractionNotice ;
    ] .

retrct_terms:RetractionNotice a owl:Class ;
    rdfs:label "Retraction Notice" ;
    rdfs:subClassOf dcterms:BibliographicResource ;
    rdfs:comment "A notice indicating the retraction of a paper." ;
    owl:sameAs wd:Q7316896 ;
    cito:discusses retrct_terms:Retraction ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty cito:confirms: ;
        owl:someValuesFrom retrct_terms:Retraction ;
    ] .
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty retrct_terms:isRetractionNoticeOf: ;
        owl:someValuesFrom retrct_terms:RetractedPaper ;
    ] .
retrct_terms:hasRetractionNotice a owl:ObjectProperty ;
    rdfs:label "has retraction notice" ;
    rdfs:domain retrct_terms:RetractedPaper ;
    rdfs:range retrct_terms:RetractionNotice ;
    owl:inverseOf retrct_terms:isRetractionNoticeOf ;
    rdfs:comment "Relates a retracted paper to its retraction notice." .

retrct_terms:isRetractionNoticeOf a owl:ObjectProperty ;
    rdfs:label "is retraction notice of" ;
    rdfs:domain retrct_terms:RetractionNotice ;
    rdfs:range retrct_terms:RetractedPaper ;
    owl:sameAs cito:retracts ;
    rdfs:comment "Relates a retraction notice to its retracted paper" .

retrct_terms:RetractionReason a owl:Class ;
    rdfs:label "retraction reason" ;
    rdfs:comment "(one of) the reason(s) stated for retraction" ;
    cito:isConfirmedBy retrct_terms:Retraction ;
    cito:refutes retrct_terms:RetractedPaper .

cito:confirms a owl:Class ;
    rdfs:label "confirms" .

cito:retracts a owl:Class ;
    rdfs:label "retracts" .
